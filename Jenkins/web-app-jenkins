pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                dotnet restore src\CombatAnalysis.WebApp\CombatAnalysis.WebApp.csproj
                dotnet build src\CombatAnalysis.WebApp\CombatAnalysis.WebApp.csproj
            }
        }
        stage('Tests') {
            steps {
                dotnet test tests\CombatAnalysis.Parser.Tests\CombatAnalysis.Parser.Tests.csproj
            }
        }
    }
}
